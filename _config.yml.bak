# Site settings 
title: "Home- Rohit Sarang"
description: "My CV!!"

# Build settings
markdown: kramdown
sass:
  sass_dir: _sass
  style: compressed

# Resume settings
resume_avatar:                  "true"
resume_name:                    "Rohit Sarang"
resume_title:                   "Budding Chief Product Officer"

# used for the contact button in _layouts/resume.html
resume_contact_email:           "hello@rohitsarang.com"

# the next two items are used for schema itemprops in _layouts/resume.html
resume_contact_telephone:       "+91 97111 94611"
resume_contact_address:         "Dwarka, New Delhi"

resume_header_contact_info:     "Dwarka, New Delhi, India • +91 97111-94611 • hello@rohitsarang.com"
display_header_contact_info:    "false"

resume_header_intro: "<p>Hi there! I am Rohit, an engineer turned banker and payments professional. I am working with SBI Cards as Assistant Vice President, managing Pricing, Network and Products. I love creating innovative products, playing with technology, building tools and have deep interest in finance. Do reach out to me at <b>hello@rohitsarang.com</b> to know more. Lets meet over a coffee (err.. a Zoom call) and explore opportunities!</p>"

# use "yes" to display the email contact button,
# "no" to display an "I'm not looking for work" message,
# or remove the resume_looking_for_work option entirely
# to leave blank
resume_looking_for_work:        "yes"

# Decide which sections to use
# comment out to hide
resume_section_experience:      true
resume_section_education:       true
# resume_section_projects:        false
resume_section_skills:          true
resume_section_recognition:     true
# resume_section_links:           true
# resume_section_associations:    false
resume_section_interests:       true

# Resume social links
# uncomment the options you wish to display, and add your own URL
resume_social_links:

  resume_facebook_url:          "https://www.facebook.com/rohit200abc"
  resume_linkedin_url:          "https://www.linkedin.com/in/rohitsarang"
  resume_instagram_url:         "https://www.instagram.com/thecruelcritic/"
  #resume_website_url:           "http://joelglovier.com"
  #resume_medium_url:            "https://medium.com/@jglovier"
resume_print_social_links:      true

# Design settings
resume_theme:                   default

